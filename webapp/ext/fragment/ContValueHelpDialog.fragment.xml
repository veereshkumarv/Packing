<core:FragmentDefinition
	xmlns="sap.ui.comp.valuehelpdialog"
	xmlns:m="sap.m"
	xmlns:fb="sap.ui.comp.filterbar"
	xmlns:core="sap.ui.core">
	<ValueHelpDialog
		title="Please Select a Container"
		ok=".onValueHelpOkPress"
		cancel=".onValueHelpCancelPress"
		key="serialNumber"
		descriptionKey="serialNumber"
		supportMultiselect="false">
		<filterBar>
			<fb:FilterBar advancedMode="true" search=".onFilterBarSearch">
				<fb:filterGroupItems>
					<fb:FilterGroupItem
						groupName="__$INTERNAL$"
						name="serialNumber"
						label="Serial Number"
						visibleInFilterBar="true">
						<fb:control>
							<m:Input name="serialNumber" />
						</fb:control>
					</fb:FilterGroupItem>
				</fb:filterGroupItems>
			</fb:FilterBar>
		</filterBar>
	</ValueHelpDialog>
</core:FragmentDefinition>